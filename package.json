  "devDependencies": {
    "@types/node": "^8.9.4",
    "@types/which": "^1.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "1.11.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "typescript": "^2.7.2"
